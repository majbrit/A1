options{	static = false;	output_directory = "parser";}PARSER_BEGIN(ProgramScanner)package parser;  public class ProgramScanner {} // parser class specificationPARSER_END(ProgramScanner)//SKIP ://{ 	//	" " | "\t" | "\r" | "\n"	//|	//	< "//" (~["\n"])* "\n" >//}void Scan() :{}{ 	<UPPERCASE>		<LI>			<END>	// end}// token specificationTOKEN : {		< INTEGER : (["0"-"9"])+ >  	|   		< UPPERCASE : ["A"-"Z"] (["A"-"Z"])* >  	|		< LOWERCASE : ["a"-"z"] (["a"-"z"])* >	|		< END : " " | "\t" | "\r" | "\n" >	//|		//< LOI : ( LOWERCASE | INTEGER )* >	|		< LI : (LOWERCASE INTEGER | INTEGER LOWERCASE) LOI >								}